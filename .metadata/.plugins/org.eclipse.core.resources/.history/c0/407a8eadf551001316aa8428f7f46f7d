package Model;

import java.util.ArrayList;

import javax.persistence.ManyToMany;

import play.db.jpa.Model;

public class Tag extends Model{
	public String title;
	@ManyToMany
	public ArrayList<Note>notes;
	@ManyToMany
	public ArrayList<Video>videos;
	public Tag()
	{
		notes= new ArrayList<>();
	}
	public Tag(String title)
	{
		this.title=title;
	}

}
